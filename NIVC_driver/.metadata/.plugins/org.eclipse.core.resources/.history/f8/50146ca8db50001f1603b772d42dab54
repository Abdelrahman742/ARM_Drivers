    else if (IRQ_Num == 135)
    {
        NVIC_PRI33_REG = (NVIC_PRI0_REG & 0x1FFFFFFF) | (IRQ_Priority << 29);
    }
    else if (IRQ_Num == 136)
    {
        NVIC_PRI34_REG = (NVIC_PRI0_REG & 0xFFFFFF1F) | (IRQ_Priority << 5);
    }
    else if (IRQ_Num == 137)
    {
        NVIC_PRI34_REG = (NVIC_PRI0_REG & 0xFFFF1FFF) | (IRQ_Priority << 13);
    }
    else if (IRQ_Num == 138)
    {
        NVIC_PRI34_REG = (NVIC_PRI0_REG & 0xFF1FFFFF) | (IRQ_Priority << 21);
    }
}
void NVIC_EnableException(NVIC_ExceptionType Exception_Num)
{

}
void NVIC_DisableException(NVIC_ExceptionType Exception_Num)
{

}
