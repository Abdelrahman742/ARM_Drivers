#include "NVIC.h"

void NVIC_EnableIRQ(NVIC_IRQType IRQ_Num)
{
    //EN0
    if (IRQ_Num < 32)
    {
        NVIC_EN0_REG |= (1 << IRQ_Num);
    }
    //EN1
    else if (IRQ_Num < 64)
    {
        NVIC_EN1_REG |= (1 << (IRQ_Num - 32));
    }
    //EN2
    else if (IRQ_Num < 96)
    {
        NVIC_EN2_REG |= (1 << (IRQ_Num - 64));
    }
    //EN3
    else if (IRQ_Num < 128)
    {
        NVIC_EN3_REG |= (1 << (IRQ_Num - 96));
    }
    //EN4
    else
    {
        NVIC_EN4_REG |= (1 << (IRQ_Num - 128));
    }
}
void NVIC_DisableIRQ(NVIC_IRQType IRQ_Num)
{
    //DIS0
    if (IRQ_Num < 32)
    {
        NVIC_DIS0_REG |= (1 << IRQ_Num);
    }
    //DIS1
    else if (IRQ_Num < 64)
    {
        NVIC_DIS1_REG |= (1 << (IRQ_Num - 32));
    }
    //DIS2
    else if (IRQ_Num < 96)
    {
        NVIC_DIS2_REG |= (1 << (IRQ_Num - 64));
    }
    //DIS3
    else if (IRQ_Num < 128)
    {
        NVIC_DIS3_REG |= (1 << (IRQ_Num - 96));
    }
    //DIS4
    else
    {
        NVIC_DIS4_REG |= (1 << (IRQ_Num - 128));
    }
}
void NVIC_SetPriorityIRQ(NVIC_IRQType IRQ_Num,
                         NVIC_IRQPriorityType IRQ_Priority)
{
    if (IRQ_Num == 0)
    {
        NVIC_PRI0_REG = (NVIC_PRI0_REG & 0xFFFFFF1F) | (IRQ_Priority << 5);
    }
    else if (IRQ_Num == 1)
    {
        NVIC_PRI0_REG = (NVIC_PRI0_REG & 0xFFFF1FFF) | (IRQ_Priority << 13);
    }
    else if (IRQ_Num == 2)
    {
        NVIC_PRI0_REG = (NVIC_PRI0_REG & 0xFF1FFFFF) | (IRQ_Priority << 21);
    }
    else if (IRQ_Num == 3)
    {
        NVIC_PRI0_REG = (NVIC_PRI0_REG & 0x1FFFFFFF) | (IRQ_Priority << 29);
    }
    else if (IRQ_Num == 4)
    {
        NVIC_PRI1_REG = (NVIC_PRI0_REG & 0xFFFFFF1F) | (IRQ_Priority << 5);
    }
    else if (IRQ_Num == 5)
    {
        NVIC_PRI1_REG = (NVIC_PRI0_REG & 0xFFFF1FFF) | (IRQ_Priority << 13);
    }
    else if (IRQ_Num == 6)
    {
        NVIC_PRI1_REG = (NVIC_PRI0_REG & 0xFF1FFFFF) | (IRQ_Priority << 21);
    }
    else if (IRQ_Num == 7)
    {
        NVIC_PRI1_REG = (NVIC_PRI0_REG & 0x1FFFFFFF) | (IRQ_Priority << 29);
    }
    else if (IRQ_Num == 8)
    {
        NVIC_PRI2_REG = (NVIC_PRI0_REG & 0xFFFFFF1F) | (IRQ_Priority << 5);
    }
    else if (IRQ_Num == 9)
    {
        NVIC_PRI2_REG = (NVIC_PRI0_REG & 0xFFFF1FFF) | (IRQ_Priority << 13);
    }
    else if (IRQ_Num == 10)
    {
        NVIC_PRI2_REG = (NVIC_PRI0_REG & 0xFF1FFFFF) | (IRQ_Priority << 21);
    }
    else if (IRQ_Num == 11)
    {
        NVIC_PRI2_REG = (NVIC_PRI0_REG & 0x1FFFFFFF) | (IRQ_Priority << 29);
    }
    else if (IRQ_Num == 12)
    {
        NVIC_PRI3_REG = (NVIC_PRI0_REG & 0xFFFFFF1F) | (IRQ_Priority << 5);
    }
    else if (IRQ_Num == 13)
